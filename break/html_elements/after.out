                  addValidationError('vacations', 0, 'Origin cannot be same as destination');
                  passedValidation = false;
              }

              if (passedValidation && new Date(fromDate).getTime() >= new Date(toDate).getTime()) {
                  addValidationError('vacations', 4, 'Choose a later date');
                  passedValidation = false;
              }
              
              if (passedValidation && differenceInDays(fromDate, toDate) > 28) {
                  addValidationError('vacations', 4, 'Maximum 28 days');
                  passedValidation = false;
              }

              if (!passedValidation) {
                  return;
              }
            
