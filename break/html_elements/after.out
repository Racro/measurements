      var hashString = function (str) {
          let hash = 0;
          for (let i = 0, len = str.length; i < len; i++) {
              let chr = str.charCodeAt(i);
              hash = (hash << 5) - hash + chr;
              hash |= 0; // Convert to 32bit integer
          }
          return hash;
      }

      var differenceInDays = function(d1, d2) {
          var date1 = new Date(d1);
          var date2 = new Date(d2);
          var differenceInTime = date2.getTime() - date1.getTime();
          
          return differenceInTime / (1000 * 3600 * 24);
      }

      var getHotelSuggestions = function (query, locale = 'en_US') {
          query = encodeURI(query);
          return new Promise(resolve => {
              $.getJSON('https://lookup.hotels.com/egta/suggest/champion/json?locale=' + locale + '&boostConfig=config-boost-champion&excludeLpa=true&providerInfoTypes=LOCAL&query=' + query, function (data) {
                  resolve(data);
              })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      throw new Error('Request failed for lookup.hotels.com: ' + err);
                  });
          });
      }

      var getCarSuggestions = function (query, locale = 'en_US') {
          query = encodeURI(query);
          return new Promise(resolve => {
              $.getJSON('https://www.travelextras-delta.com/api/v4/typeahead/' + query + '?client=Homepage&dest=true&format=json&lob=CARS&locale=' + locale + '&maxresults=' + maxSearchResults, function (data) {
                  resolve(data);
              })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      throw new Error('Request failed for lookup.hotels.com: ' + err);
                  });
          });
      }

